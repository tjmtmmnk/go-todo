# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
    id: ID!
    itemName: String!
    done: Boolean!
    user: User!
    startAt: String
    endAt: String
    createdAt: String
    updatedAt: String
}

type User {
    id: ID!
    name: String!
    nickname: String
    createdAt: String
    updatedAt: String
}

type Query {
    todos: [Todo!]!
}

input NewTodo {
    text: String!
    userId: String!
}

type Mutation {
    createTodo(input: NewTodo!): Todo!
}
